{
  "name": "mcpbox-keystone",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "dev": "env-cmd keystone dev",
    "start": "prisma migrate resolve --applied 20251003172613_ && keystone start --with-migrations",
    "build": "keystone build",
    "docker": "docker-compose up -d",
    "migrate": "env-cmd keystone prisma migrate dev --create-only",
    "lint": "eslint src --max-warnings=0 --fix",
    "act": "act -j build --secret-file .env --env-file .env --var-file .env --container-architecture linux/amd64"
  },
  "dependencies": {
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/cloudinary": "^8.0.0",
    "@keystone-6/core": "^6.0.0",
    "@keystone-6/fields-document": "^9.0.0",
    "lodash": "^4.17.21",
    "slate-dom": "^0.117.4",
    "typescript": "^5.5.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.0"
  }
}
